
public class LoopsAndMore {
	public int sumEveryThird(int limit)
	{
		int sum = 0;
		for(int i = 3; i < limit; i=+3) {
			sum = i;
		}
		return sum;
	}
	
	
	
	public String printJava(int count)
	{
		return null;
	}
	
	
	public int largestPowerOf2(int target)
	{
		return 0;
	}
	
